-- start screen

function start_menu()
	game_state="menu"
	music(0)
end

function update_menu()
	if (btnp(❎)) start_game(0)
	if (btnp(🅾️)) start_game(1)
end

function draw_menu()
 cls()
	print("tiny tanks", 42, 4, 7)
	print("press ❎ to start", 28, 100, frame/4)
end

function start_game(plyr_num)
 selected_player = plyr_num
 game_state="game"
	init_tank(0)
	init_tank(1)
	music(-1)
end
