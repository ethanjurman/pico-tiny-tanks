debug=""
game_state="menu"
frame=0

function update_game()
 if (selected_player==0) then
	update_tank(1) 	
	update_tank(0)
 else
	update_tank(0) 	
	update_tank(1) 	
 end
end

function _init()
	start_menu()
end

function _update()
 frame+=1
 if game_state=="game" then
  update_game()
 end
 if game_state=="menu" then
  update_menu()
 end
end

function _draw()
 if game_state=="game" then
	 cls(5)
	 palt(0,false)
	 map()
	 --print("debug:"..debug)
	 print("tiny tanks", 42, 4, 7)
		draw_tank(0)
		draw_tank(1)
	end
	if game_state=="menu" then
	 draw_menu()
	end
end
